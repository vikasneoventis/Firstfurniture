<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\TrackingImport\Block\Adminhtml\Widget\Menu" name="trackingimport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\TrackingImport\Block\Adminhtml\Tools" name="adminhtml.trackingimport.tools">
                <block class="Xtento\TrackingImport\Block\Adminhtml\Tools\Import" name="adminhtml.trackingimport.tools.import" template="Xtento_TrackingImport::tools/import.phtml"/>
                <block class="Xtento\TrackingImport\Block\Adminhtml\Tools\Export" name="adminhtml.trackingimport.tools.export" template="Xtento_TrackingImport::tools/export.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>

