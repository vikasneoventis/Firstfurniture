{
  "name": "turiknox/magento2-webgains-tracking",
  "description": "A simple Magento module that will allow you to integrate Webgains tracking code on the checkout success page.",
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "php": "~5.6.5|7.0.2|~7.0.6",
    "magento/module-backend": "100.1.*",
    "magento/module-checkout": "100.1.*",
    "magento/module-store": "100.1.*",
    "magento/framework": "100.1.*",
    "magento/module-customer": "100.1.*",
    "magento/module-sales": "100.1.*"
  },
  "type": "magento2-module",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Turiknox/magento2-webgains-tracking"
    }
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Turiknox\\Webgains\\": ""
    }
  }
}